    "@ceramicnetwork/streamid": "^5.3.0-nightly.20240606002730.0",
    "@coinbase/onchainkit": "^0.33.2",
    "@duneanalytics/client-sdk": "^0.2.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/static": "^7.0.3",
    "@graphiql/create-fetcher": "^0.1.0",
    "@graphql-tools/schema": "^10.0.4",
    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.90.1",
    "@supabase/supabase-js": "^2.38.1",
    "@useorbis/db-sdk": "^0.0.40-alpha",
    "ace-builds": "^1.33.1",
    "better-sqlite3": "^9.3.0",
    "bs58": "^5.0.0",
    "change-case": "^5.4.2",
    "codeco": "^1.2.1",
    "cross-env": "^7.0.3",
