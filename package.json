    "dids": "^4.0.4",
    "ethers": "^6.11.1",
    "fastify": "^4.26.2",
    "fastify-gql": "^5.8.2",
    "formidable": "^3.5.1",
    "fs": "^0.0.1-security",
    "graphiql": "^3.3.1",
    "graphql": "^16.8.2",
    "graphql-scalars": "^1.23.0",
    "graphql-ws": "^5.16.0",
    "graphql-yoga": "^5.7.0",
